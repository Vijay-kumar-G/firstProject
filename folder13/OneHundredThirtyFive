class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int[] arr= new int[2];
        int count=0;
        HashSet<Integer> ss = new HashSet<>();
        for(int i=0;i<nums.length;i++){
            if(ss.contains(nums[i])){
                arr[count]=nums[i];
                count++;
            }
            else{
                ss.add(nums[i]);
            }
        }

        return arr;
    }
}
